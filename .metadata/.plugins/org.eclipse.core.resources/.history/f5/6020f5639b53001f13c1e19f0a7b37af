function ajax_data(){
	var http;
	http = new XMLHttpRequest();
	http.onreadystatechange = function(){
		if(http.readyState == 4 && http.status==200){
			html_code(JSON.parse(this.response)); //문자열이여서 JSON.parse써야됨,왜 배열로 안바뀜?
		}
	}
	http.open("GET","./coupon_api.do",true);
	http.send();
}
ajax_data();

//JSON데이터를 HTML로 출력
function html_code(data){
	var ea=data.length;
	var result = "";
	document.getElementById("total").append(ea);
	//console.log(data); //배열왜안나와아아랑알	
	data.forEach(function(a,b,c){
		console.log(a);
	});
}