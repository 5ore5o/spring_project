function ajax_data(){
	var http,result;
	http = new XMLHttpRequest();
	http.onreadystatechange = function(){
		if(http.readyState == 4 && http.status==200){
			html_code(JSON.parse(this.response)); //문자열이여서 JSON.parse써야됨
		}
	}
	http.open("GET","./coupon_api.do",true);
	http.send();
}
ajax_data();

//JSON데이터를 HTML로 출력
